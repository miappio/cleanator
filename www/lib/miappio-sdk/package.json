{
  "name": "miappio-sdk",
  "version": "1.0.35",
  "description": "miapp.io client SDK",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "./node_modules/.bin/grunt default; ./node_modules/.bin/karma start test/karma.dist-tests.conf.js",
    "bump": "./node_modules/.bin/grunt bump",
    "postpublish": "",
    "test-mocha": "./node_modules/.bin/mocha -R dot ./test/test-*.js",
    "test-karma": "./node_modules/.bin/karma start test/karma.unit-tests.conf.js",
    "test-karma-dist": "./node_modules/.bin/karma start test/karma.dist-tests.conf.js",
    "test-karma-watch": "./node_modules/.bin/karma start test/karma.unit-tests.conf.js --no-single-run  --auto-watch --reporters progress --browsers PhantomJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miappio/miappio-sdk.git"
  },
  "keywords": [
    "miapp.io",
    "mobile",
    "angular",
    "integration"
  ],
  "author": "miapp.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miappio/miappio-sdk/issues"
  },
  "homepage": "https://github.com/miappio/miappio-sdk#readme",
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "grunt": "*",
    "grunt-bump": "*",
    "grunt-cli": "*",
    "grunt-contrib-concat": "*",
    "grunt-contrib-uglify": "*",
    "grunt-typescript-project": "*",
    "jasmine-core": "*",
    "karma": "*",
    "karma-chrome-launcher": "*",
    "karma-coverage": "*",
    "karma-html-reporter": "*",
    "karma-jasmine": "*",
    "karma-jasmine-html-reporter-livereload": "*",
    "karma-phantomjs-launcher": "*",
    "typescript": "^2.0.6"
  },
  "dependencies": {
    "pouchdb": "^6.0.7"
  }
}
