{
  "name": "miappio-sdk",
  "version": "1.0.9",
  "description": "miapp.io client SDK",
  "main": "dist/miapp.io.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run test-karma",
    "prepublish": "grunt default; grunt bump",
    "postpublish": "",
    "test-mocha": "./node_modules/.bin/mocha -R dot ./test/test-*.js",
    "test-karma": "./node_modules/.bin/karma start test/karma.unit-tests.conf.js",
    "test-karma-watch": "./node_modules/.bin/karma start test/karma.unit-tests.conf.js --no-single-run  --no-auto-watch --reporters progress --browsers PhantomJS"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miappio/miappio-sdk.git"
  },
  "keywords": [
    "miapp.io",
    "mobile",
    "angular",
    "integration"
  ],
  "author": "miapp.io",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/miappio/miappio-sdk/issues"
  },
  "homepage": "https://github.com/miappio/miappio-sdk#readme",
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "chai": "^3.5.0",
    "grunt": "^1.0.1",
    "grunt-bump": "^0.8.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-concat": "*",
    "grunt-contrib-uglify": "^2.0.0",
    "jasmine-core": "^2.4.1",
    "jsdom": "9.5.0",
    "jsdom-global": "2.1.0",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-html-reporter": "*",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter-livereload": "*",
    "karma-phantomjs-launcher": "^1.0.1",
    "mocha-jsdom": "^1.1.0"
  },
  "dependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.0.2"
  }
}
