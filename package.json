        "gulp-cheerio": "^0.6.3",
        "gulp-concat": "^2.2.0",
        "gulp-minify-css": "^0.3.0",
        "gulp-preprocess": "^2.0.0",
        "gulp-rename": "^1.2.0",
        "gulp-sass": "^2.0.4",
        "gulp-util": "^2.2.20",
        "http-server": "^0.9.0",
