{
  "name": "cleanator",
  "version": "17.1.10",
  "description": "cleanator",
  "author": "mlefree",
  "homepage": "https://miapp.github.io/cleanator",
  "main": "www/index.html",
  "dependencies": {
    "bower": "*",
    "codeclimate-test-reporter": "*",
    "gulp": "^3.9.0",
    "gulp-angular-gettext": "^2.2.0",
    "gulp-bump": "^2.4.0",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.2.0",
    "gulp-minify-css": "^0.3.0",
    "gulp-preprocess": "^2.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-sass": "^2.0.4",
    "gulp-util": "^2.2.20",
    "http-server": "^0.9.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-html-reporter": "*",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter-livereload": "*",
    "karma-phantomjs-launcher": "^1.0.1",
    "phantomjs": "*",
    "shelljs": "^0.7.5"
  },
  "cordovaPlugins": [
    "cordova-plugin-device",
    "cordova-plugin-console",
    "cordova-plugin-whitelist",
    "cordova-plugin-splashscreen",
    "cordova-plugin-statusbar",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [
    "android"
  ],
  "scripts": {
    "bower:install": "./node_modules/.bin/bower install",
    "gulp:default": "npm version patch; ./node_modules/.bin/gulp",
    "test": "./node_modules/.bin/karma start tests/karma.unit-tests.conf.js",
    "test:old": "./node_modules/.bin/karma start tests/karma.unit-tests.conf.js --reporters coverage",
    "test:watch": "./node_modules/.bin/karma start tests/karma.unit-tests.conf.js --no-single-run  --no-auto-watch --reporters progress --browsers PhantomJS",
    "start": "./node_modules/.bin/gulp 'config-test-js'; ./node_modules/.bin/http-server -a 0.0.0.0 -p 8080 ./www/",
    "dev:serve_notused": "ionic serve",
    "dev:build": "npm version patch; ./node_modules/.bin/gulp; ionic build android",
    "dev:run": "ionic run android --device"
  },
  "devDependencies": {
    "jasmine-ajax": "^3.3.1",
    "karma-jasmine-ajax": "^0.1.13"
  }
}
