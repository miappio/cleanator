        "karma-chrome-launcher": "^1.0.1",
        "karma-coverage": "^1.1.0",
        "karma-html-reporter": "*",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-ajax": "^0.1.13",
        "karma-jasmine-html-reporter-livereload": "*",
        "karma-phantomjs-launcher": "^1.0.1",
        "phantomjs": "*",
