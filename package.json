    "karma": "^1.1.1",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.0",
    "karma-html-reporter": "*",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter-livereload": "*",
